<launch>

    <!-- <arg name="bagfile" default="/media/psf/Home/Google\ Drive/GAVLAB/data/trucks/9_1_17/run0_with_remote_vehicle_state_with_drtk_fix.bag"/> -->

    <!-- <arg name="bagfile" default="/media/psf/Home/Google\ Drive/GAVLAB/data/trucks/8_15_17/bucket_radar_test.bag"/> -->

    <arg name="bagpath" default="/media/psf/Home/Google Drive/GAVLAB/data/trucks/9_1_17"/>

    <arg name="bagfile" default="$(arg bagpath)/run0_with_remote_vehicle_state_with_drtk_fix"/>

    <!-- <arg name="bagfile" default="$(arg bagpath)/changing_gap_1_2017-09-04-14-45-41"/> -->


    <!-- <arg name="bagfile" default="$(arg bagpath)/changing_gap_2_2017-09-04-14-49-48"/> -->
    <!-- <arg name="bagfile" default="$(arg bagpath)/changing_gap_3_2017-09-04-14-54-12"/> -->
    <!-- <arg name="bagfile" default="$(arg bagpath)/changing_gap_3_2017-09-04-14-58-21"/> -->
    <!-- <arg name="bagfile" default="$(arg bagpath)/control_edits_test_1_2017-09-04-15-45-11"/> -->
    <!-- <arg name="bagfile" default="$(arg bagpath)/control_edits_test_2_2017-09-04-15-55-58"/> -->
    <!-- <arg name="bagfile" default="$(arg bagpath)/control_edits_test_3_2017-09-04-16-01-15"/> -->
    <!-- <arg name="bagfile" default="$(arg bagpath)/estimation_run1_2017-09-04-14-27-32"/> -->
    <!-- <arg name="bagfile" default="$(arg bagpath)/estimation_run2_2017-09-04-14-32-26"/> -->
    <!-- <arg name="bagfile" default="$(arg bagpath)/static_2017-09-04-15-20-51"/> -->
    <!-- <arg name="bagfile" default="$(arg bagpath)/static_control_1_2017-09-04-14-42-10"/> -->


    <!-- rosbag play -->
    <param name="use_sim_time" value="true"/>
    <node name="rosbag" pkg="rosbag" type="play" args="-s 20 --clock -r 3.0 '$(arg bagfile).bag'" >
        <!-- <remap from="/drtk/drtkOutput2" to="/drtk_node/drtkOutput"/> -->
        <remap from="/range_kf_node/rangeEstimationOutput" to="/range_kf_node/rangeEstimationOutput_live"/>
    </node>

    <!-- decompress image -->
    <node name="republish" type="republish" pkg="image_transport" output="screen" args="compressed in:=/axis/image_raw out:=/axis_decompressed" />

    <!-- range estimator -->
    <include file="$(find cacc_range_estimation)/launch/cacc_range_estimation.launch"/>

    <!-- radar overlay -->
    <include file="$(find radar_visualization)/launch/radar_visualization.launch"/>

    <!-- image view -->
    <node pkg="image_view" type="image_view" name="image_view" output="screen" required="true" args="image:=/radar_visualization/image_overlay"/>

</launch>
